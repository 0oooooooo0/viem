{
  "name": "viem",
  "description": "TypeScript (& JavaScript) Interface for Ethereum",
  "version": "0.0.0",
  "dependencies": {},
  "files": [
    "/actions",
    "/chains",
    "/dist",
    "/providers",
    "/types",
    "/window"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./actions/account": {
      "module": "./actions/account/dist/viem-actions-account.esm.js",
      "default": "./actions/account/dist/viem-actions-account.cjs.js"
    },
    "./actions/public": {
      "module": "./actions/public/dist/viem-actions-public.esm.js",
      "default": "./actions/public/dist/viem-actions-public.cjs.js"
    },
    "./actions/wallet": {
      "module": "./actions/wallet/dist/viem-actions-wallet.esm.js",
      "default": "./actions/wallet/dist/viem-actions-wallet.cjs.js"
    },
    "./chains": {
      "module": "./chains/dist/viem-chains.esm.js",
      "default": "./chains/dist/viem-chains.cjs.js"
    },
    "./providers/account": {
      "module": "./providers/account/dist/viem-providers-account.esm.js",
      "default": "./providers/account/dist/viem-providers-account.cjs.js"
    },
    "./providers/network": {
      "module": "./providers/network/dist/viem-providers-network.esm.js",
      "default": "./providers/network/dist/viem-providers-network.cjs.js"
    },
    "./providers/test": {
      "module": "./providers/test/dist/viem-providers-test.esm.js",
      "default": "./providers/test/dist/viem-providers-test.cjs.js"
    },
    "./providers/wallet": {
      "module": "./providers/wallet/dist/viem-providers-wallet.esm.js",
      "default": "./providers/wallet/dist/viem-providers-wallet.cjs.js"
    },
    "./window": {
      "module": "./window/dist/viem-window.esm.js",
      "default": "./window/dist/viem-window.cjs.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "actions/account/index.ts",
      "actions/public/index.ts",
      "actions/wallet/index.ts",
      "chains.ts",
      "index.ts",
      "providers/account/index.ts",
      "providers/network/index.ts",
      "providers/test/index.ts",
      "providers/wallet/index.ts",
      "window.ts"
    ]
  },
  "main": "dist/viem.cjs.js",
  "module": "dist/viem.esm.js",
  "types": "dist/viem.cjs.d.ts",
  "license": "WAGMIT",
  "repository": "wagmi-dev/viem",
  "author": "moxey.eth",
  "ethereum": "moxey.eth",
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ]
}
