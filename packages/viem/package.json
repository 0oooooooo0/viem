{
  "name": "viem",
  "description": "TypeScript (& JavaScript) Interface for Ethereum",
  "version": "0.0.0",
  "dependencies": {
    "@noble/hashes": "^1.1.2"
  },
  "files": [
    "/actions",
    "/chains",
    "/dist",
    "/providers",
    "/types",
    "/utils",
    "/window"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./actions": {
      "module": "./actions/dist/viem-actions.esm.js",
      "default": "./actions/dist/viem-actions.cjs.js"
    },
    "./chains": {
      "module": "./chains/dist/viem-chains.esm.js",
      "default": "./chains/dist/viem-chains.cjs.js"
    },
    "./providers": {
      "module": "./providers/dist/viem-providers.esm.js",
      "default": "./providers/dist/viem-providers.cjs.js"
    },
    "./utils": {
      "module": "./utils/dist/viem-utils.esm.js",
      "default": "./utils/dist/viem-utils.cjs.js"
    },
    "./window": {
      "module": "./window/dist/viem-window.esm.js",
      "default": "./window/dist/viem-window.cjs.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "actions/index.ts",
      "chains.ts",
      "index.ts",
      "providers/index.ts",
      "utils/index.ts",
      "window.ts"
    ]
  },
  "main": "dist/viem.cjs.js",
  "module": "dist/viem.esm.js",
  "types": "dist/viem.cjs.d.ts",
  "license": "WAGMIT",
  "repository": "wagmi-dev/viem",
  "author": "moxey.eth",
  "ethereum": "moxey.eth",
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ]
}
